 	u32 sd_pads_50mhz_cnt;
 
 	switch (index) {
+	case 0:
+		if (cpu_is_mx6q()) {
+			sd_pads_200mhz = cm_fx6_q_sd1_200mhz;
+			sd_pads_100mhz = cm_fx6_q_sd1_100mhz;
+			sd_pads_50mhz  = cm_fx6_q_sd1_50mhz;
